{
  "name": "node-sql",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F @node-sql/db db:push",
    "db:studio": "turbo -F @node-sql/db db:studio",
    "db:generate": "turbo -F @node-sql/db db:generate",
    "db:migrate": "turbo -F @node-sql/db db:migrate"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "server": "^1.0.42",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "^1.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tsdown": "^0.15.5",
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.15.0"
}
